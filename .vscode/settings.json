{
  "workbench.colorTheme": "Ayu Mirage",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.expo": true,
    "**/*.tsbuildinfo": true,
    "**/Pods": true,
    "Podfile": true,
    "**/dist": true,
    "**/dist-electron": true,
    "**/tsconfig.tsbuildinfo": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    // Prevent showing in streams
    ".env": true,
    "**/.env": true,
    "**/.env.example": true
  },
  "files.exclude": {
    "**/.env": true,
    "**/Info.plist": true,
    "**/tsconfig.tsbuildinfo": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/tray-animations": true,
  }
}